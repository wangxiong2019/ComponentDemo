<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wzmt.usercomponent" >

    <application
        android:name="com.wzmt.commonlib.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/usercomponent_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.UserAc"/>
        <activity android:name=".activity.needlogin.MyInfoAc"/>
    </application>
</manifest>
